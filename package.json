{
  "name": "wp-terms-navigation",
  "version": "0.0.8",
  "description": "Wordpress theme",
  "private": true,
  "main": "dist/main.js",
  "style": "dist/main.css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benignware-labs/wp-terms-navigation.git"
  },
  "keywords": [
    "wordpress",
    "theme"
  ],
  "author": "Rafael Nowrotek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benignware-labs/wp-terms-navigation/issues"
  },
  "homepage": "https://github.com/benignware-labs/wp-terms-navigation#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@wordpress/browserslist-config": "^2.2.2",
    "autoprefixer": "^9.1.5",
    "chokidar-cli": "^1.2.1",
    "cpx": "^1.5.0",
    "glob": "^7.1.4",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.0",
    "postcss-focus-within": "^3.0.0",
    "query-ast": "^1.0.2",
    "rtlcss": "^2.4.0",
    "sass-extract": "^2.1.0",
    "sass-variable-parser": "^1.2.2",
    "scss-parser": "^1.0.3"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "scripts": {
    "watch": "chokidar \"src/**/*.*\" -c \"npm run build\" --initial",
    "build:polyfill": "cpx './node_modules/mdn-polyfills/**/*.*' dist/polyfill",
    "build:js": "npx babel src/js/terms-navigation.js --out-file dist/terms-navigation.js",
    "build:scss": "node-sass src/scss/terms-navigation.scss dist/terms-navigation.css --output-style expanded",
    "build": "run-p \"build:*\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.9.0",
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "mdn-polyfills": "^5.18.0",
    "popper.js": "^1.15.0"
  }
}
